{
  "name": "dependency-injection-container",
  "version": "1.0.0",
  "description": "",
  "author": "Alexey Vykhrystyuk <AVykhrystyuk@gmail.com>",
  "license": "ISC",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es2019.js",
  "typings": "dist/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "run-s build:clean build:types bundle:rollup",
    "build:prod": "BABEL_ENV=production run-s build:clean build:types bundle:rollup",
    "build:clean": "rimraf dist",
    "build:types": "tsc --emitDeclarationOnly",
    "bundle:rollup": "run-s bundle:rollup:es5 bundle:rollup:cjs bundle:rollup:es2019",
    "bundle:rollup:es5": "BUILD_TYPE=es5 rollup -c",
    "bundle:rollup:cjs": "BUILD_TYPE=cjs rollup -c",
    "bundle:rollup:es2019": "BUILD_TYPE=es2019 rollup -c"
  },
  "keywords": [
    "dependency injection container",
    "dependency",
    "injection",
    "container",
    "di container",
    "di",
    "ioc",
    "injector"
  ],
  "files": [
    "dist/*",
    "README.md"
  ],
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.0",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/preset-env": "^7.5.0",
    "@babel/preset-typescript": "^7.3.3",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.3",
    "rollup": "^1.16.3",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.1.1",
    "typescript": "^3.5.2"
  }
}
